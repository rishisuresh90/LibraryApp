	<!-- script references -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.0/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.0/jquery.validate.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script>
      $(document).ready(function() {
        console.log('helooo');
        $("#basic-form").validate({
          rules: {
            name: {
              required: true,
              minlength: 3
            },
            email: {
              required: true,
              email: true
            },
            password: {
              required: true,
              minlength: 8,
            },
            confirmpassword: {
              required: true,
              minlength: 8,
              equalTo: '[name="password"]'
            }
          },
          messages: {
            name: {
              name: "Name should atleast 3 characters"
            },
            email: {
              email: "The email should be in the format: abc@domain.tld"
            },
            password: {
              minlength: "The password should be atleast 8 characters"
            },
            confirmpassword: {
              confirmpassword: "Enter confirmpassword same as password"
            }
          },
          submitHandler: function(form) {
            form.submit();
          }
        });
      });
    </script>
